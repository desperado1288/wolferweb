{
  "name": "wolfernode",
  "version": "1.0.0",
  "description": "Redux + React Web Server for WolferX",
  "main": "server.js",
  "author": "WolferX",
  "license": "MIT",
  "keywords": [
    "Node",
    "Redux",
    "React",
    "Webpack"
  ],
  "engines": {
    "node": ">=5.0.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rm -rf public",
    "compile": "node -r dotenv/config --harmony bin/compile",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "lint": "eslint . ./",
    "lint:fix": "npm run lint -- --fix"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon -r dotenv/config bin/server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "node -r dotenv/config bin/server"
    },
    "test": {
      "command": "node -r dotenv/config ./node_modules/karma/bin/karma start bin/karma.js",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wolferian/wolferweb.git"
  },
  "dependencies": {
    "async": "^1.5.1",
    "better-npm-run": "0.0.5",
    "body-parser": "^1.14.2",
    "bootstrap-sass": "^3.3.6",
    "colors": "^1.1.2",
    "compression": "^1.6.0",
    "debug": "^2.2.0",
    "dotenv": "^1.2.0",
    "express": "^4.13.3",
    "history": "^1.17.0",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.0",
    "jquery": "^2.2.0",
    "mongoose": "^4.3.4",
    "morgan": "^1.6.1",
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "react-motion": "^0.3.1",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "redux-actions": "^0.9.0",
    "redux-logger": "^2.3.1",
    "redux-simple-router": "^1.0.2",
    "redux-thunk": "^1.0.3",
    "request": "^2.67.0",
    "serve-favicon": "^2.3.0",
    "socket.io": "^1.4.0",
    "swig": "^1.4.2",
    "underscore": "^1.8.3",
    "xml2js": "^0.4.15",
    "yargs": "^3.31.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.2.3",
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.2.0",
    "chai-immutable": "^1.5.3",
    "css-loader": "^0.23.1",
    "cssnano": "^3.4.0",
    "debug": "^2.2.0",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-config-standard-react": "^1.2.1",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.14.0",
    "eslint-plugin-standard": "^1.3.1",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.8.5",
    "forever-monitor": "^1.7.0",
    "fs-extra": "^0.26.4",
    "html-webpack-plugin": "^1.7.0",
    "http": "0.0.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.19",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chai-sinon": "^0.1.5",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-spec-reporter": "0.0.23",
